<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="lib_ui_CTickView">
        <attr name="lib_ui_ctv_color" format="color" />
        <attr name="lib_ui_ctv_colorCircle" format="color" />
        <attr name="lib_ui_ctv_strokeWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="lib_ui_LrcView">
        <attr name="lib_ui_lrc_textColor" format="color" />
        <attr name="lib_ui_lrc_textColorCur" format="color" />
        <attr name="lib_ui_lrc_timeColor" format="color" />
        <attr name="lib_ui_lrc_textSize" format="dimension" />
        <attr name="lib_ui_lrc_textSizeCur" format="dimension" />
        <attr name="lib_ui_lrc_timeSize" format="dimension" />
        <attr name="lib_ui_lrc_padding" format="dimension" />
        <attr name="lib_ui_lrc_minScale" format="float" />
        <attr name="lib_ui_lrc_maxScale" format="float" />
    </declare-styleable>

    <declare-styleable name="lib_ui_ToggleButton">
        <attr name="lib_ui_tbtn_colorNormal" format="color" />
        <attr name="lib_ui_tbtn_colorLight" format="color" />
        <attr name="lib_ui_tbtn_colorPadding" format="color" />
        <attr name="lib_ui_tbtn_padding" format="dimension" />
        <attr name="lib_ui_tbtn_duration" format="integer" />
    </declare-styleable>

    <declare-styleable name="lib_ui_ArcMenu">
        <attr name="lib_ui_am_position">
            <enum name="left_top" value="0" />
            <enum name="left_bottom" value="1" />
            <enum name="right_top" value="2" />
            <enum name="right_bottom" value="3" />
        </attr>
        <attr name="lib_ui_am_radius" format="dimension" />
    </declare-styleable>

    <!-- 飘星自定义属性 -->
    <declare-styleable name="lib_ui_HeartLayout">
        <attr name="lib_ui_hl_initX" format="dimension" />
        <attr name="lib_ui_hl_initY" format="dimension" />
        <attr name="lib_ui_hl_xRand" format="dimension" />
        <attr name="lib_ui_hl_animLengthRand" format="dimension" />
        <attr name="lib_ui_hl_xPointFactor" format="dimension" />
        <attr name="lib_ui_hl_animLength" format="dimension" />
        <attr name="lib_ui_hl_heartWidth" format="dimension" />
        <attr name="lib_ui_hl_heartHeight" format="dimension" />
        <attr name="lib_ui_hl_bezierFactor" format="integer" />
        <attr name="lib_ui_hl_animDuration" format="integer" />
    </declare-styleable>

    <declare-styleable name="lib_ui_ScrollTab">
        <attr name="lib_ui_stab_type">
            <enum name="view" value="0" />
            <enum name="view_group" value="1" />
        </attr>
        <attr name="lib_ui_stab_titles" format="string" />
        <attr name="lib_ui_stab_avag" format="boolean" />
        <attr name="lib_ui_stab_padding" format="dimension" />
        <attr name="lib_ui_stab_indicatorType">
            <enum name="trend" value="0" />
            <enum name="translation" value="1" />
            <enum name="none" value="2" />
        </attr>
        <attr name="lib_ui_stab_indicatorColor" format="color" />
        <attr name="lib_ui_stab_indicatorWidth" format="dimension" />
        <attr name="lib_ui_stab_indicatorWeight" format="dimension" />
        <attr name="lib_ui_stab_indicatorRadius" format="dimension" />
        <attr name="lib_ui_stab_indicatorPadding" format="dimension" />
        <attr name="lib_ui_stab_duration" format="integer" />
    </declare-styleable>

    <declare-styleable name="lib_ui_ShadowLayout">
        <attr name="lib_ui_sl_shadowed" format="boolean" />
        <attr name="lib_ui_sl_shadowDistance" format="dimension" />
        <attr name="lib_ui_sl_shadowAngle" format="integer" />
        <attr name="lib_ui_sl_shadowRadius" format="dimension" />
        <attr name="lib_ui_sl_shadowColor" format="color" />
    </declare-styleable>

    <declare-styleable name="lib_ui_SettingProgressView">
        <attr name="lib_ui_spv_count" format="integer" />
        <attr name="lib_ui_spv_colorSelect" format="color" />
        <attr name="lib_ui_spv_colorUnselect" format="color" />
        <attr name="lib_ui_spv_radiusSmall" format="dimension" />
        <attr name="lib_ui_spv_radiusBig" format="dimension" />
        <attr name="lib_ui_spv_radiusSpace" format="dimension" />
    </declare-styleable>

    <declare-styleable name="lib_ui_ConvenientBanner">
        <attr name="lib_ui_cb_canLoop" format="boolean" />
        <attr name="lib_ui_cb_ppadding" format="dimension" />
        <attr name="lib_ui_cb_layout" format="reference" />
    </declare-styleable>
</resources>